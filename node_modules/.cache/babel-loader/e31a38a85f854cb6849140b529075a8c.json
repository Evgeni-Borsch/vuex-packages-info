AAA,GAAA,KAAA,eAAA;AACA,aAAA,KAAA,YAAA,CAAA,KAAA,CAAA,IAAA,EAAA,EAAA,CAAA;AACA,KATA;AAUA,IAAA,YAVA,0BAUA;AAAA;;AACA,aAAA,KAAA,WAAA,CAAA,MAAA,CAAA,UAAA,IAAA,EAAA;AACA,eAAA,IAAA,CAAA,IAAA,CAAA,KAAA,CAAA,KAAA,CAAA,MAAA,CAAA;AACA,OAFA,CAAA;AAGA;AAdA,IAjCA;AAmDA,EAAA,OAAA,kCACA,UAAA,CAAA,CAAA,iBAAA,CAAA,CADA;AAEA,IAAA,SAFA,qBAEA,IAFA,EAEA;AACA,WAAA,UAAA,GAAA,IAAA;AACA,KAJA;AAKA,IAAA,UALA,sBAKA,IALA,EAKA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA,WAAA,KAAA,GAAA,IAAA,CAAA,IAAA;AACA,WAAA,MAAA,GAAA,IAAA,CAAA,IAAA;AACA,WAAA,SAAA,GAAA,CAAA,KAAA,SAAA;AACA;AAVA;AAnDA,CAAA","sourcesContent":["<template>\n  <v-container class=\"col-md-6\">\n    <v-row>\n      <v-text-field \n        dark\n        v-model=\"search\"\n        label=\"Введите название пакета\"\n        append-icon=\"mdi-magnify\"\n        @keyup.enter=\"search = ''\"  \n      >\n      </v-text-field>     \n    </v-row>\n    <v-row class=\"mainSection\">\n      <v-simple-table \n        fixed-header \n        dark\n        dense \n        class=\"table_content\"\n      >\n          <template v-slot:default>\n              <thead>\n                  <tr>\n                    <th >\n                        <h2 class=\"table_content\"> Package name </h2>\n                    </th>\n                    <th >\n                        <h2 class=\"table_content\"> Type </h2>\n                    </th>\n                  </tr>\n              </thead>              \n              <tbody >\n                <tr\n                  class=\"trTable\"\n                  v-for=\"(item, index) in paginatedTable\"\n                  :key=\"index\"\n                  :search=\"search\"\n                  @click=\"getPackage(item)\"\n                >\n                      <td>{{ item.name }}</td>\n                      <td>{{ item.type }}</td>\n                </tr>\n              </tbody>\n          </template>\n      </v-simple-table>\n      \n      <modal-window \n        class=\"modalWindow\" \n        v-if=\"showModal\" \n        :title=\"title\"\n        :isType=\"isType\"        \n      >\n      </modal-window>\n    </v-row>\n\n    <v-row class=\"col-md-12\" justify=\"center\">\n      <v-col class=\"page\" \n        v-for=\"page in pages\" \n        :key=\"page\"\n        :class=\"{'pageSelected': page === pageNumber}\"\n        @click=\"pageClick(page)\"\n      >\n        {{ page }}\n      </v-col>\n    </v-row>\n  </v-container>\n\n</template>\n\n<script>\nimport {mapActions, mapGetters} from 'vuex';\nimport modalWindow from './modalWindow'\n\n\nexport default {\n  data() {\n    return {\n      showModal: false,\n      isActive: false,\n      packagesPerPage: 10,\n      pageNumber: 1,\n      reveal: false,\n      title: {\n        type: Object,\n        required: false\n      },\n      isType: '',\n      search: ''\n    }\n\n  },\n  components: {\n    modalWindow\n  },\n\n  mounted() {\n    this.showDateInTable();\n  },\n  \n  watch:{\n    paginatedTable(newValue){\n      if(newValue.length < 10){\n        this.pageNumber = 1\n      }\n    }\n  },\n  \n  computed: {\n    ...mapGetters([\"allPackages\"]),\n    pages() {\n      return Math.ceil(this.allPackages.length / 10);\n    },\n    paginatedTable() {\n      let from = (this.pageNumber - 1)* this.packagesPerPage;\n      let to = from + this.packagesPerPage;\n      return this.filtredNames.slice(from,to);\n    },\n    filtredNames() {\n      return this.allPackages.filter((item) => {\n        return item.name.match(this.search)\n      })\n    }\n  },  \n  \n  \n  methods: {\n    ...mapActions([\"showDateInTable\"]),\n    pageClick(page) {\n      this.pageNumber = page;\n    },\n    getPackage(item){\n      console.log(item);\n      this.title = item.name;\n      this.isType = item.type;\n      this.showModal = !this.showModal;\n    },\n   \n  }\n};\n\n</script>\n\n<style>\n.page {\n  padding: 10px;\n  color: black;\n  margin: 5px;\n  cursor: pointer;\n  text-align: center;\n  border-radius: 30%;\n}\n.page:hover {\n  background: rgb(17, 10, 10);\n  transition: 0.2s;\n  color: white;\n}\n.pageSelected {\n  background: rgb(17, 10, 10);\n  color: white;\n}\n.modalWindow{\n  max-width: 344px;\n  height: 300px;\n  overflow-y: scroll;\n}\n.trTable{\n  cursor: pointer;\n}\n.mainSection {\n  display: flex;\n  justify-content: space-between;\n}\n</style>"],"sourceRoot":"src/components"}]}